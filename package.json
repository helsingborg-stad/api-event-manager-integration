{
    "name": "@helsingborg-stad/api-event-manager-integration",
    "version": "3.1.0",
    "description": "Import and display events from Event Manager API.",
    "main": "",
    "scripts": {
        "test": "jest --coverage --maxWorkers=50%",
        "dev": "vite",
        "build": "vite build",
        "build:dev": "vite build --mode development",
        "watch": "vite build --watch --mode development",
        "format": "npx biome format ./source",
        "format:write": "npx biome format ./source --write",
        "lint": "npx biome lint ./source",
        "lint:write": "npx biome lint ./source --write"
    },
    "author": "Jonatan Hanson, Sebastian Thulin",
    "license": "MIT",
    "browserslist": [
        "> 1%",
        "not dead"
    ],
    "peerDependencies": {
        "react": "19.0.0",
        "react-dom": "19.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.5",
        "@helsingborg-stad/hbg-react": "^1.0.21",
        "@types/jest": "^30.0.0",
        "@types/react": "^16",
        "@types/react-dom": "^16",
        "es6-promise": "^4.2.8",
        "immutability-helper": "^3.1.1",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "prop-types": "^15.8.1",
        "react": "^16.14.0",
        "react-day-picker": "^7.4.8",
        "react-dom": "^16.14.0",
        "react-onclickoutside": "^6.9.1",
        "sass-embedded": "^1.93.3",
        "scroll-into-view-if-needed": "^2.2.29",
        "set-query-string": "^2.2.0",
        "ts-jest": "^29.4.5",
        "typescript": "^5.9.2",
        "uuid": "^8.3.2",
        "vite": "^7.1.2",
        "vite-config-factory": "^1.0.1",
        "@vitejs/plugin-react": "^5.1.2"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|ts)$": "ts-jest"
        },
        "collectCoverageFrom": [
            "source/**/*.js",
            "source/**/*.ts"
        ],
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "testMatch": [
            "**/*.test.(js|ts)"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(.*\\.mjs$))"
        ]
    }
}