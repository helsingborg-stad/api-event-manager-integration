{
    "name": "helsingborg-stad/api-event-manager-integration",
    "description": "Import and display events from Event Manager API.",
    "type": "wordpress-plugin",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jonatan Hanson",
            "email": "jonatan.hanson@hiq.se"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/helsingborg-stad/acf-export-manager.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "helsingborg-stad/acf-export-manager": ">=1.0.0",
        "justinrainbow/json-schema": "^5.2",
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "EventManagerIntegration\\": "source/php/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "make-pot": "wp i18n make-pot . languages/event-integration-sv_SE.pot --domain=event-integration --allow-root --include=source/php/,AcfFields/",
        "update-po": "wp i18n update-po languages/event-integration-sv_SE.pot languages/event-integration-sv_SE.po --allow-root",
        "make-mo": "wp i18n make-mo languages/event-integration-sv_SE.po languages/event-integration-sv_SE.mo --allow-root"
    },
    "require-dev": {
        "lucatume/function-mocker": "^1.3",
        "10up/wp_mock": "^0.5.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}